{
  "name": "ai-legacy-frontend",
  "version": "0.0.0",
  "description": "CDP Frontend Template",
  "sideEffects": false,
  "main": ".server/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.13.1"
  },
  "scripts": {
    "build": "run-s build:frontend",
    "build:frontend": "NODE_ENV=production webpack",
    "dev": "run-p frontend:watch server:watch",
    "dev:debug": "run-p frontend:watch server:debug",
    "frontend:watch": "NODE_ENV=development webpack --watch",
    "lint": "run-s lint:js lint:scss",
    "lint:fix": "eslint . --fix",
    "lint:js": "eslint .",
    "lint:scss": "stylelint \"src/**/*.scss\" --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore",
    "pretest": "npm run lint",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,njk --legacy-watch src/index.js",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,njk --legacy-watch src/index.js",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --use-strict ."
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@defra/hapi-tracing": "^1.0.0",
    "@elastic/ecs-pino-format": "^1.5.0",
    "@hapi/catbox-memory": "^6.0.2",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/hapi": "^21.3.12",
    "@hapi/inert": "^7.1.0",
    "@hapi/vision": "^7.0.3",
    "@hapi/yar": "^11.0.2",
    "@vitest/ui": "^3.2.4",
    "aws-embedded-metrics": "^4.2.0",
    "convict": "^6.2.4",
    "cssnano": "^7.0.6",
    "cssnano-preset-default": "^7.0.6",
    "date-fns": "^4.1.0",
    "global-agent": "^3.0.0",
    "govuk-frontend": "^5.10.2",
    "hapi-pino": "^12.1.0",
    "hapi-pulse": "^3.0.1",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsdom": "^26.1.0",
    "lodash": "^4.17.21",
    "nunjucks": "^3.2.4",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "undici": "^6.21.1",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "cheerio": "^1.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^9.30.1",
    "neostandard": "^0.12.1",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "postcss-load-config": "^6.0.1",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.80.6",
    "sass-loader": "^16.0.3",
    "source-map-loader": "^5.0.0",
    "stylelint": "^16.10.0",
    "stylelint-config-gds": "^2.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.96.1",
    "webpack-assets-manifest": "^5.2.1",
    "webpack-cli": "^5.1.4"
  }
}
